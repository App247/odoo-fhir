        <act_window id="hc_res_care_plan_action"
                    name="HC Care Plans"
                    res_model="hc.res.care.plan"
                    view_mode="tree,form"
                    domain=""
                    context=""
                    limit=""/>

        <menuitem id="hc_care_plan.menu_care_plan"
                  name="Care Plans"
                  web_icon="hc_care_plan,static/description/icon.png"
                  parent="hc_base.menu_clinic_care"
                  action="hc_res_care_plan_action"
                  groups="hc_base.group_hc_base_manager"
                  sequence="10"/>


        <!-- Care Plan Category Views -->

        <record id="hc_vs_care_plan_category_view_search" model="ir.ui.view">
            <field name="name">Care Plan Category Search</field>
            <field name="model">hc.vs.care.plan.category</field>
            <field name="arch" type="xml">
                <search string="Care Plan Categories">
                    <field name="name"/>
                    <field name="code"/>
                    <field name="definition"/>
                    <group>
                        <filter name="group_by_parent" string="parent" context="{'group_by': 'contains_id'}"/>
                    </group>
                </search>
            </field>
        </record>

        <record id="hc_vs_care_plan_category_view_tree" model="ir.ui.view">
            <field name="name">Care Plan Category Tree</field>
            <field name="model">hc.vs.care.plan.category</field>
            <field name="arch" type="xml">
                <tree string="Care Plan Categories" default_order="name, code">
                    <field name="name"/>
                    <field name="code"/>
                    <field name="definition"/>
                    <field name="contains_id"/>
                    <field name="source_id" invisible="0"/>
                </tree>
            </field>
        </record>

        <record id="hc_vs_care_plan_category_view_form" model="ir.ui.view">
            <field name="name">Care Plan Category Form</field>
            <field name="model">hc.vs.care.plan.category</field>
            <field name="arch" type="xml">
                <form string="Care Plan Category">
                    <sheet>
                        <h1>
                            <field name="name" default_focus="1" placeholder="Name (e.g., Biological Substance)"/>
                        </h1>
                        <field name="code" placeholder="Code (e.g., biological)"/>
                        <field name="definition" placeholder="Definition"/>
                        <field name="comments" placeholder="Comments"/>
                        <group>
                            <group>
                                <field name="contains_id" invisible="0"/>
                                <field name="level" placeholder="An integer (e.g., 1, 2)"/>
                                <field name="is_abstract"/>
                                <field name="version" placeholder="e.g., v2.5"/>
                            </group>
                            <group>
                                <field name="source_id" placeholder="e.g., U.S. Census Bureau"/>
                                <field name="system" widget="url" placeholder="e.g., www.census.gov/"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <act_window id="hc_vs_care_plan_category_action"
                    name="HC Care Plan Categories"
                    res_model="hc.vs.care.plan.category"
                    view_mode="tree,form"
                    domain=""
                    context=""
                    limit=""/>

        <menuitem id="hc_care_plan.menu_care_plan_cat"
                  name="Care Plan Category"
                  parent="hc_care_plan.menu_care_plan"
                  action="hc_vs_care_plan_category_action"
                  groups="hc_base.group_hc_base_manager"
                  sequence="10"/>

        <!-- External reference -->

        <record id="hc_res_procedure_view_form_inherited" model="ir.ui.view">
            <field name="name">Procedure Form Inherit</field>
            <field name="model">hc.res.procedure</field>
            <field name="inherit_id" ref="hc_procedure.hc_res_procedure_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='based_on_ids']//form//group//field[@name='based_on_type']" position="after">
                    <field name="based_on_care_plan_id" class="oe_edit_only" attrs="{'invisible': [('based_on_type','!=','care_plan')], 'required': [('based_on_type','=','care_plan')]}"/>
                </xpath>
                <!-- <xpath expr="//field[@name='report_ids']" position="after">
                    <tree>
