<odoo>
    <data>

        <!-- Care Plan Views -->

        <record id="hc_res_care_plan_view_search" model="ir.ui.view">
            <field name="name">Care Plan Search</field>
            <field name="model">hc.res.care.plan</field>
            <field name="arch" type="xml">
                <search string="Care Plans">
                    <field name="title"/>
                </search>
            </field>
        </record>

        <record id="hc_res_care_plan_view_tree" model="ir.ui.view">
            <field name="name">Care Plan Tree</field>
            <field name="model">hc.res.care.plan</field>
            <field name="arch" type="xml">
                <tree string="Care Plans">
                    <field name="title"/>
                </tree>
            </field>
        </record>

        <record id="hc_res_care_plan_view_form" model="ir.ui.view">
            <field name="name">Care Plan Form</field>
            <field name="model">hc.res.care.plan</field>
            <field name="arch" type="xml">
                <form string="Care Plan">
                    <sheet>
                        <h1>
                            <field name="title" default_focus="1" placeholder="Title"/>
                        </h1>
                        <field name="description"/>
                        <group>
                            <group string="Event">
                                <field name="subject_name"/>
                                <field name="subject_type" class="oe_edit_only"/>
                                <field name="subject_patient_id" class="oe_edit_only" attrs="{'invisible': [('subject_type','!=','patient')], 'required': [('subject_type','=','patient')]}"/>
                                <field name="subject_group_id" class="oe_edit_only" attrs="{'invisible': [('subject_type','!=','group')], 'required': [('subject_type','=','group')]}"/>
                                <field name="start_date"/>
                                <field name="end_date"/>
                                <field name="category_ids" widget="many2many_tags"/>
                            </group>
                            <group string="Status">
                                <field name="status"/>
                                <field name="intent"/>
                            </group>
                            <group string="Context">
                                <field name="context_name"/>
                                <field name="context_type" class="oe_edit_only"/>
                                <field name="context_encounter_id" class="oe_edit_only" attrs="{'invisible': [('context_type','!=','encounter')], 'required': [('context_type','=','encounter')]}"/>
                                <field name="context_episode_of_care_id" class="oe_edit_only" attrs="{'invisible': [('context_type','!=','episode_of_care')], 'required': [('context_type','=','episode_of_care')]}"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Identifiers">
                                <field name="identifier_ids" mode="tree">
                                    <tree>
                                        <field name="is_preferred"/>
                                        <field name="name"/>
                                        <field name="value"/>
                                        <field name="type_id"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <div class="oe_title oe_left">
                                                <h1>
                                                    <field name="name" placeholder="Identifier"/>
                                                </h1>
                                            </div>
                                            <group>
                                                <group>
                                                    <field name="value"/>
                                                    <field name="type_id"/>
                                                    <field name="is_preferred"/>
                                                    <field name="is_active"/>
                                                </group>
                                                <group>
                                                    <field name="start_date" placeholder="Valid From"/>
                                                    <field name="end_date" placeholder="Valid To"/>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Definitions">
                                <field name="definition_ids" mode="tree">
                                    <tree>
                                        <field name="definition_name"/>
                                        <field name="definition_type"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group>
                                                <group>
                                                    <field name="definition_name"/>
                                                    <field name="definition_type" class="oe_edit_only"/>
                                                    <field name="definition_plan_definition_id" class="oe_edit_only" attrs="{'invisible': [('definition_type','!=','plan_definition')], 'required': [('definition_type','=','plan_definition')]}"/>
                                                    <field name="definition_questionnaire_id" class="oe_edit_only" attrs="{'invisible': [('definition_type','!=','questionnaire')], 'required': [('definition_type','=','questionnaire')]}"/>
                                                </group>
                                                <group>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Based On">
                                <field name="based_on_ids" mode="tree">
                                    <tree>
                                        <field name="based_on_id"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group>
                                                <group>
                                                    <field name="based_on_id" placeholder="Care Plan"/>
                                                </group>
                                                <group>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Replace">
                                <field name="replaces_ids" mode="tree">
                                    <tree>
                                        <field name="replaces_id"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group>
                                                <group>
                                                    <field name="replaces_id" placeholder="Care Plan"/>
                                                </group>
                                                <group>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Parts Of">
                                <field name="part_of_ids" mode="tree">
                                    <tree>
                                        <field name="part_of_id"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group>
                                                <group>
                                                    <field name="part_of_id" placeholder="Care Plan"/>
                                                </group>
                                                <group>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Authors">
                                <field name="author_ids" mode="tree">
                                    <tree>
                                        <field name="author_name"/>
                                        <field name="author_type"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group>
                                                <group>
                                                    <field name="author_name"/>
                                                    <field name="author_type" class="oe_edit_only"/>
                                                    <field name="author_patient_id" class="oe_edit_only" attrs="{'invisible': [('author_type','!=','patient')], 'required': [('author_type','=','patient')]}"/>
                                                    <field name="author_practitioner_id" class="oe_edit_only" attrs="{'invisible': [('author_type','!=','practitioner')], 'required': [('author_type','=','practitioner')]}"/>
                                                    <field name="author_related_person_id" class="oe_edit_only" attrs="{'invisible': [('author_type','!=','related_person')], 'required': [('author_type','=','related_person')]}"/>
                                                    <field name="author_organization_id" class="oe_edit_only" attrs="{'invisible': [('author_type','!=','organization')], 'required': [('author_type','=','organization')]}"/>
                                                    <field name="author_care_team_id" class="oe_edit_only" attrs="{'invisible': [('author_type','!=','care_team')], 'required': [('author_type','=','care_team')]}"/>
                                                </group>
                                                <group>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Care Teams">
                                <field name="care_team_ids" mode="tree">
                                    <tree>
                                        <field name="care_team_id"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group>
                                                <group>
                                                    <field name="care_team_id"/>
                                                </group>
                                                <group>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Address">
                                <field name="addresses_ids" mode="tree">
                                    <tree>
                                        <field name="addresses_id"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group>
                                                <group>
                                                    <field name="addresses_id" placeholder="Condition"/>
                                                </group>
                                                <group>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Supporting Info">
                                <field name="supporting_info_ids" mode="tree">
                                    <tree>
                                        <field name="supporting_info_name"/>
                                        <field name="supporting_info_type"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group>
                                                <group>
                                                    <field name="supporting_info_type"/>
                                                    <field name="supporting_info_name"/>
                                                </group>
                                                <group>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Goals">
                                <field name="goal_ids" mode="tree">
                                    <tree>
                                        <field name="goal_id"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group>
                                                <group>
                                                    <field name="goal_id"/>
                                                </group>
                                                <group>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Notes">
                                <field name="note_ids" mode="tree">
                                    <tree>
                                        <field name="text"/>
                                        <field name="author_type"/>
                                        <field name="author_name"/>
                                        <field name="recorded_date"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <div>
                                                <field name="text" placeholder="Annotation"/>
                                            </div>
                                            <group>
                                                <group>
                                                    <field name="author_name"/>
                                                    <field name="author_type" class="oe_edit_only"/>
                                                    <field name="author_string" class="oe_edit_only" attrs="{'invisible': [('author_type','!=','string')]}"/>
                                                    <field name="author_practitioner_id" class="oe_edit_only" attrs="{'invisible': [('author_type','!=','practitioner')]}"/>
                                                    <field name="author_patient_id" class="oe_edit_only" attrs="{'invisible': [('author_type','!=','patient')]}"/>
                                                    <field name="author_related_person_id" class="oe_edit_only" attrs="{'invisible': [('author_type','!=','related_person')]}"/>
                                                </group>
                                                <group>
                                                    <field name="recorded_date"/>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Activities">
                                <field name="activity_ids" mode="tree">
                                    <tree>
                                        <field name="reference_name"/>
                                        <field name="reference_type"/>
                                        <field name="detail_id"/>
                                        <field name="outcome_codeable_concept_ids" widget="many2many_tags"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group>
                                                <group>
                                                    <field name="reference_name"/>
                                                    <field name="reference_type" class="oe_edit_only"/>
                                                    <field name="reference_appointment_id" class="oe_edit_only" attrs="{'invisible': [('reference_type','!=','appointment')], 'required': [('reference_type','=','appointment')]}"/>
                                                    <field name="reference_communication_request_id" class="oe_edit_only" attrs="{'invisible': [('reference_type','!=','communication_request')], 'required': [('reference_type','=','communication_request')]}"/>
                                                    <field name="reference_device_request_id" class="oe_edit_only" attrs="{'invisible': [('reference_type','!=','device_request')], 'required': [('reference_type','=','device_request')]}"/>
                                                    <field name="reference_medication_request_id" class="oe_edit_only" attrs="{'invisible': [('reference_type','!=','medication_request')], 'required': [('reference_type','=','medication_request')]}"/>
                                                    <field name="reference_nutrition_order_id" class="oe_edit_only" attrs="{'invisible': [('reference_type','!=','nutrition_order')], 'required': [('reference_type','=','nutrition_order')]}"/>
                                                    <field name="reference_task_id" class="oe_edit_only" attrs="{'invisible': [('reference_type','!=','task')], 'required': [('reference_type','=','task')]}"/>
                                                    <field name="reference_procedure_request_id" class="oe_edit_only" attrs="{'invisible': [('reference_type','!=','procedure_request')], 'required': [('reference_type','=','procedure_request')]}"/>
                                                    <field name="reference_vision_prescription_id" class="oe_edit_only" attrs="{'invisible': [('reference_type','!=','vision_prescription')], 'required': [('reference_type','=','vision_prescription')]}"/>
                                                    <field name="reference_request_group_id" class="oe_edit_only" attrs="{'invisible': [('reference_type','!=','request_group')], 'required': [('reference_type','=','request_group')]}"/>
                                                </group>
                                                <group>
                                                    <field name="detail_id"/>
                                                    <field name="outcome_codeable_concept_ids" widget="many2many_tags"/>
                                                </group>
                                            </group>
                                            <notebook>
                                                <page string="Outcome References">
                                                    <field name="outcome_reference_ids" mode="tree">
                                                        <tree>
                                                            <field name="outcome_reference_type"/>
                                                            <field name="outcome_reference_name"/>
                                                        </tree>
                                                        <form>
                                                            <sheet>
                                                                <group>
                                                                    <group>
                                                                        <field name="outcome_reference_type"/>
                                                                        <field name="outcome_reference_name"/>
                                                                    </group>
                                                                    <group>
                                                                    </group>
                                                                </group>
                                                            </sheet>
                                                        </form>
                                                    </field>
                                                </page>
                                                <page string="Progress">
                                                    <field name="progress_ids" mode="tree">
                                                        <tree>
                                                            <field name="text"/>
                                                            <field name="author_type"/>
                                                            <field name="author_name"/>
                                                            <field name="recorded_date"/>
                                                        </tree>
                                                        <form>
                                                            <sheet>
                                                                <div>
                                                                    <field name="text" placeholder="Annotation"/>
                                                                </div>
                                                                <group>
                                                                    <group>
                                                                        <field name="author_name"/>
                                                                        <field name="author_type" class="oe_edit_only"/>
                                                                        <field name="author_string" class="oe_edit_only" attrs="{'invisible': [('author_type','!=','string')]}"/>
                                                                        <field name="author_practitioner_id" class="oe_edit_only" attrs="{'invisible': [('author_type','!=','practitioner')]}"/>
                                                                        <field name="author_patient_id" class="oe_edit_only" attrs="{'invisible': [('author_type','!=','patient')]}"/>
                                                                        <field name="author_related_person_id" class="oe_edit_only" attrs="{'invisible': [('author_type','!=','related_person')]}"/>
                                                                    </group>
                                                                    <group>
                                                                        <field name="recorded_date"/>
                                                                    </group>
                                                                </group>
                                                            </sheet>
                                                        </form>
                                                    </field>
                                                </page>
                                            </notebook>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Status History">
                                <field name="status_history_ids" mode="tree">
                                    <tree>
                                        <field name="status" readonly="1"/>
                                        <field name="start_date"/>
                                        <field name="end_date"/>
                                        <field name="time_diff_day"/>
                                        <field name="time_diff_hour"/>
                                        <field name="time_diff_min"/>
                                        <field name="time_diff_sec"/>
                                        <field name="write_uid" invisible="0"/>
                                    </tree>
                                </field>
                            </page>
                            <page string="Intent History">
                                <field name="intent_history_ids" mode="tree">
                                    <tree>
                                        <field name="intent" readonly="1"/>
                                        <field name="start_date"/>
                                        <field name="end_date"/>
                                        <field name="time_diff_day"/>
                                        <field name="time_diff_hour"/>
                                        <field name="time_diff_min"/>
                                        <field name="time_diff_sec"/>
                                        <field name="write_uid" invisible="0"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="hc_care_plan_activity_detail_view_form" model="ir.ui.view">
            <field name="name">Plan Activity Detail Form</field>
            <field name="model">hc.care.plan.activity.detail</field>
            <field name="arch" type="xml">
                <form string="Plan Activity Detail">
                    <sheet>
                        <h1>
                            <field name="name" default_focus="1" placeholder="Name"/>
                        </h1>
                        <group>
                            <group>
                                <field name="definition_name"/>
                                <field name="definition_type" class="oe_edit_only"/>
                                <field name="definition_plan_definition_id" class="oe_edit_only" attrs="{'invisible': [('definition_type','!=','plan_definition')], 'required': [('definition_type','=','plan_definition')]}"/>
                                <field name="definition_activity_definition_id" class="oe_edit_only" attrs="{'invisible': [('definition_type','!=','activity_definition')], 'required': [('definition_type','=','activity_definition')]}"/>
                                <field name="definition_questionnaire_id" class="oe_edit_only" attrs="{'invisible': [('definition_type','!=','questionnaire')], 'required': [('definition_type','=','questionnaire')]}"/>
                                <newline/>
                                <field name="product_name"/>
                                <field name="product_type" class="oe_edit_only"/>
                                <field name="product_code_id" class="oe_edit_only" attrs="{'invisible': [('product_type','!=','code')], 'required': [('product_type','=','code')]}"/>
                                <field name="product_medication_id" class="oe_edit_only" attrs="{'invisible': [('product_type','!=','medication')], 'required': [('product_type','=','medication')]}"/>
                                <field name="product_substance_id" class="oe_edit_only" attrs="{'invisible': [('product_type','!=','substance')], 'required': [('product_type','=','substance')]}"/>
                                <newline/>
                                <field name="category"/>
                                <field name="code_id"/>
                                <field name="reason_code_ids" widget="many2many_tags"/>
                                <field name="is_prohibited"/>
                                <field name="location_id"/>
                                <field name="daily_amount"/>
                                <field name="daily_amount_uom_id" attrs="{'invisible': [('daily_amount','=',0)], 'required': [('daily_amount','!=',0)]}"/>
                                <field name="quantity"/>
                                <field name="quantity_uom_id" attrs="{'invisible': [('quantity','=',0)], 'required': [('quantity','!=',0)]}"/>
                            </group>
                            <group string="Status">
                                <field name="status"/>
                            </group>
                            <group string="Time">
                                <field name="scheduled_name"/>
                                <field name="scheduled_type" class="oe_edit_only"/>
                                <field name="scheduled_timing_id" class="oe_edit_only" attrs="{'invisible': [('scheduled_type','!=','timing')], 'required': [('scheduled_type','=','timing')]}"/>
                                <field name="scheduled_start_date" class="oe_edit_only" attrs="{'invisible': [('scheduled_type','!=','period')], 'required': [('scheduled_type','=','period')]}"/>
                                <field name="scheduled_end_date" class="oe_edit_only" attrs="{'invisible': [('scheduled_type','!=','period')], 'required': [('scheduled_type','=','period')]}"/>
                                <field name="scheduled" class="oe_edit_only" attrs="{'invisible': [('scheduled_type','!=','string')], 'required': [('scheduled_type','=','string')]}"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Reason References">
                                <field name="reason_reference_ids" mode="tree">
                                    <tree>
                                        <field name="reason_reference_id"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group>
                                                <group>
                                                    <field name="reason_reference_id" placeholder="Condition"/>
                                                </group>
                                                <group>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Goals">
                                <field name="goal_ids" mode="tree">
                                    <tree>
                                        <field name="goal_id"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group>
                                                <group>
                                                    <field name="goal_id"/>
                                                </group>
                                                <group>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Performers">
                                <field name="performer_ids" mode="tree">
                                    <tree>
                                        <field name="performer_name"/>
                                        <field name="performer_type"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group>
                                                <group>
                                                    <field name="performer_name"/>
                                                    <field name="performer_type" class="oe_edit_only"/>
                                                    <field name="performer_practitioner_id" class="oe_edit_only" attrs="{'invisible': [('performer_type','!=','practitioner')], 'required': [('performer_type','=','practitioner')]}"/>
                                                    <field name="performer_organization_id" class="oe_edit_only" attrs="{'invisible': [('performer_type','!=','organization')], 'required': [('performer_type','=','organization')]}"/>
                                                    <field name="performer_related_person_id" class="oe_edit_only" attrs="{'invisible': [('performer_type','!=','related_person')], 'required': [('performer_type','=','related_person')]}"/>
                                                    <field name="performer_patient_id" class="oe_edit_only" attrs="{'invisible': [('performer_type','!=','patient')], 'required': [('performer_type','=','patient')]}"/>
                                                    <field name="performer_care_team_id" class="oe_edit_only" attrs="{'invisible': [('performer_type','!=','care_team')], 'required': [('performer_type','=','care_team')]}"/>
                                                </group>
                                                <group>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Status History">
                                <field name="status_history_ids" mode="tree">
                                    <tree>
                                        <field name="status" readonly="1"/>
                                        <field name="start_date"/>
                                        <field name="end_date"/>
                                        <field name="time_diff_day"/>
                                        <field name="time_diff_hour"/>
                                        <field name="time_diff_min"/>
                                        <field name="time_diff_sec"/>
                                        <field name="write_uid" invisible="0"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

