            <field name="definition">Hospital, clinic and typically inpatient claims.</field>
            <field name="source_id" ref="base.hl7fhir"/>
            <field name="system">http://hl7.org/fhir/ValueSet/claim-type</field>
        </record>

        <record id="claim_type_oral" model="hc.vs.claim.type">
            <field name="code">oral</field>
            <field name="name">Oral</field>
