<odoo>
    <data>

        <record id="hc_res_related_person_view_search" model="ir.ui.view">
            <field name="name">Related Person Search</field>
            <field name="model">hc.res.related.person</field>
            <field name="arch" type="xml">
                <search string="Related Persons">
                    <field name="person_id"/>
                    <field name="gender"/>
                    <field name="birth_date"/>
                </search>
            </field>
        </record>

        <record id="hc_res_related_person_view_tree" model="ir.ui.view">
            <field name="name">Related Person Tree</field>
            <field name="model">hc.res.related.person</field>
            <field name="arch" type="xml">
                <tree string="Related Persons">
                    <field name="person_id"/>
                    <field name="gender"/>
                    <field name="birth_date"/>
                </tree>
            </field>
        </record>

        <record id="hc_res_related_person_view_form" model="ir.ui.view">
            <field name="name">Related Person Form</field>
            <field name="model">hc.res.related.person</field>
            <field name="arch" type="xml">
                <form string="Related Person">
                    <sheet>
                        <field name="image" widget="image" class="oe_left oe_avatar" options="{'preview_image': 'image_medium', 'size': [90, 90]}"/>
                        <div class="oe_title">
                            <h1>
                                <field name="person_id" default_focus="1" placeholder="Person"/>
                            </h1>
                        </div>
                        <div name="buttons" class="oe_right oe_button_box"></div>
                        <group>
                            <group>
                                <field name="gender"/>
                                <field name="birth_date"/>
                            </group>
                            <group>
                                <field name="is_active" string="Active Record"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Patients">
                                <field name="patient_ids" mode="tree">
                                </field>
                            </page>
                            <page string="Names">
                                <field name="name_ids" mode="tree">
                                    <tree default_order="is_preferred">
                                        <field name="is_preferred"/>
                                        <field name="use"/>
                                        <field name="name"/>
                                        <field name="start_date"/>
                                        <field name="end_date"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group>
                                                <field name="is_preferred"/>
                                                <field name="person_id"/>
                                            </group>
                                            <group>
                                                <field name="first_id"/>
                                                <field name="middle_ids"/>
                                                <field name="initial_ids"/>
                                                <field name="nickname_ids"/>
                                                <field name="mother_maiden_id"/>
                                                <field name="surname_id"/>
                                                <field name="family"/>
                                                <field name="given"/>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Identifiers">
                                <field name="identifier_ids" mode="tree">
                                    <tree>
                                        <field name="is_preferred"/>
                                        <field name="name"/>
                                        <field name="value"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <div>
                                                <h1>
                                                    <field name="name" placeholder="Identifier"/>
                                                </h1>
                                            </div>
                                            <group col="4" colspan="4">
                                                <group>
                                                    <field name="value"/>
                                                    <field name="system"/>
                                                    <field name="is_preferred"/>
                                                    <field name="is_active"/>
                                                </group>
                                                <group>
                                                    <field name="code_id"/>
                                                    <field name="type_id"/>
                                                    <field name="use"/>
                                                    <field name="start_date" placeholder="Valid From"/>
                                                    <field name="end_date" placeholder="Valid To"/>
                                               </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Addresses">
                                <field name="address_ids" mode="tree">
                                    <tree>
                                        <field name="use" widget="radio"/>
                                        <field name="is_preferred"/>
                                        <field name="address_id"/>
                                        <field name="start_date"/>
                                        <field name="end_date"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group col="4" colspan="4">
                                                <field name="use" widget="radio" colspan="4"/>
                                                <field name="address_id" class="oe_no_button"/>
                                                <field name="is_preferred"/>
                                                <field name="is_active"/>
                                                <field name="start_date" placeholder="Valid From"/>
                                                <field name="end_date" placeholder="Valid To"/>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Telecom">
                                <field name="telecom_ids" mode="tree">
                                    <tree default_order="is_preferred">
                                        <field name="is_preferred"/>
                                        <field name="use"/>
                                        <field name="telecom_id"/>
                                        <field name="rank"/>
                                        <field name="start_date"/>
                                        <field name="end_date"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group>
                                                <group>
                                                    <field name="use"/>
                                                    <field name="telecom_id"/>
                                                    <field name="rank"/>
                                                </group>
                                                <group>
                                                    <field name="is_preferred"/>
                                                    <field name="is_active"/>
                                                    <field name="start_date" placeholder="Valid From"/>
                                                    <field name="end_date" placeholder="Valid To"/>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Photos">
                                <field name="photo_ids" mode="tree">
                                    <tree default_order="is_preferred">
                                        <field name="is_preferred"/>
                                        <field name="name" string="Photo Name"/>
                                        <field name="start_date"/>
                                        <field name="end_date"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group>
                                                <group>
                                                    <field name="name" string="Photo Name"/>
                                                    <field name="datas"/>
                                                </group>
                                                <group>
                                                    <field name="is_preferred"/>
                                                    <field name="is_active"/>
                                                    <field name="start_date" placeholder="Valid From"/>
                                                    <field name="end_date" placeholder="Valid To"/>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Meta">
                                <form>
                                    <sheet>
                                        <group string="Resource">
                                            <group>
                                                <field name="identifier"/>
                                                <field name="implicit_rules"/>
                                                <field name="language_id"/>
                                                <field name="meta_id"/>
                                            </group>
                                            <group>
                                            </group>
                                        </group>
                                        <group string="Domain Resource">
                                            <group>
                                                <field name="text_id"/>
                                            </group>
                                            <group>
                                            </group>
                                        </group>
                                        <notebook>
                                            <page string="Contained">
                                                <field name="contained_ids" mode="tree">
                                                    <tree>
                                                        <field name="identifier"/>
                                                        <field name="meta_id"/>
                                                        <field name="implicit_rules"/>
                                                        <field name="language_id"/>
                                                    </tree>
                                                    <form>
                                                        <sheet>
                                                            <group>
                                                                <group>
                                                                    <field name="identifier"/>
                                                                    <field name="meta_id"/>
                                                                    <field name="implicit_rules"/>
                                                                    <field name="language_id"/>
                                                                </group>
                                                                <group>
                                                                </group>
                                                            </group>
                                                        </sheet>
                                                    </form>
                                                </field>
                                            </page>
