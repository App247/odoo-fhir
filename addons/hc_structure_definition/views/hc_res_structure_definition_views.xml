                                                </group>
                                            </group>    
                                        </sheet>        
                                    </form>         
                                </field>                
                            </page>    
                            <page string="Mappings">                 
                                <field name="mapping_ids" mode="tree">               
                                    <tree>
                                        <field name="name"/>
                                        <field name="identity"/>
                                        <field name="uri"/>
                                        <field name="comment"/>          
                                    </tree>         
                                    <form>          
                                        <sheet>     
                                            <group> 
