<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

    <!-- ukp Location -->

        <record id="location_ukp" model="hc.res.location">
            <field name="identifier">ukp</field>
            <field name="status">active</field>
            <field name="name">UK Pharmacies</field>
            <field name="description">All Pharmacies in the United Kingdom covered by the National Pharmacy Association</field>
            <field name="mode">kind</field>
            <field name="type_id" ref="serv_del_role_type_pharm"/>
            <field name="physical_type_id" ref="phy_type_jdn"/>
        </record>

        <record id="location_ukp_text" model="hc.location.domain.resource.text">
            <field name="location_id" ref="location_ukp"/>
            <field name="status">generated</field>
            <field name="div">
                <!-- UK Pharmacies -->
            </field>
        </record>

    </data>
</odoo>
