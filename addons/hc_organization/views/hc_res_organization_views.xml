<odoo>
    <data>

        <!-- Organization Views -->

        <record id="hc_res_organization_view_search" model="ir.ui.view">
            <field name="name">Organization Search</field>
            <field name="model">hc.res.organization</field>
            <field name="arch" type="xml">
                <search string="Organizations">
                    <field name="partner_id"/>
                    <field name="is_active"/>
                    <field name="type_ids"/>
                    <field name="part_of_id"/>
                    <field name="company_id"/>
                </search>
            </field>
        </record>

        <record id="hc_res_organization_view_tree" model="ir.ui.view">
            <field name="name">Organization Tree</field>
            <field name="model">hc.res.organization</field>
            <field name="arch" type="xml">
                <tree string="Organizations">
                    <field name="partner_id"/>
                    <field name="is_active"/>
                    <field name="type_ids" widget="many2many_tags"/>
                    <field name="part_of_id"/>
                    <field name="company_id"/>
                </tree>
            </field>
        </record>

        <record id="hc_res_organization_view_form" model="ir.ui.view">
            <field name="name">Organization Form</field>
            <field name="model">hc.res.organization</field>
            <field name="arch" type="xml">
                <form string="Organization">
                    <sheet>
                        <h1>
                            <field name="name" default_focus="1" placeholder="Name"/>
                        </h1>
                        <group>
                            <group>
                                <field name="partner_id"/>
                                <field name="is_active"/>
                                <field name="type_ids" widget="many2many_tags"/>
                            </group>
                            <group>
                                <field name="part_of_id" placeholder="Organization"/>
                                <field name="company_id" string="Odoo Company"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Contacts">
                                <field name="contact_ids" mode="tree">
                                    <tree>
                                        <field name="person_id"/>
                                        <field name="purpose_id"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <h1>
                                                <field name="person_id" placeholder="Name"/>
                                            </h1>
                                            <group>
                                                <group>
                                                    <field name="purpose_id"/>
                                                </group>
                                                <group>
                                                    <field name="address_id"/>
                                                </group>
                                            </group>
                                            <notebook>
                                                <page string="Telecom">
                                                    <field name="telecom_ids" mode="tree">
                                                        <tree default_order="is_preferred,rank">
                                                            <field name="is_preferred"/>
                                                            <field name="rank"/>
                                                            <field name="use"/>
                                                            <field name="telecom_id"/>
                                                            <field name="start_date"/>
                                                            <field name="end_date"/>
                                                        </tree>
                                                        <form>
                                                            <sheet>
                                                                <h1>
                                                                    <field name="telecom_id"/>
                                                                </h1>
                                                                <field name="use" widget="radio" options="{'horizontal': true}"/>
                                                                <group>
                                                                    <group>
                                                                        <field name="rank"/>
                                                                        <field name="is_preferred"/>
                                                                        <field name="is_active"/>
                                                                        <field name="start_date" placeholder="Valid From"/>
                                                                        <field name="end_date" placeholder="Valid To"/>
                                                                    </group>
                                                                </group>
                                                            </sheet>
                                                        </form>
                                                    </field>
                                                </page>
                                            </notebook>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Identifiers">
                                <field name="identifier_ids" mode="tree">
                                    <tree default_order="is_preferred">
                                        <field name="is_preferred"/>
                                        <field name="name"/>
                                        <field name="value"/>
                                        <field name="start_date"/>
                                        <field name="end_date"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <h1>
                                                <field name="name" placeholder="Identifier"/>
                                            </h1>
                                            <group col="4" colspan="4">
                                                <group>
                                                    <field name="value"/>
                                                    <field name="system"/>
                                                    <field name="is_preferred"/>
                                                    <field name="is_active"/>
                                                </group>
                                                <group>
                                                    <field name="start_date" placeholder="Valid From"/>
                                                    <field name="end_date" placeholder="Valid To"/>
                                               </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Aliases">
                                <field name="alias_ids" mode="tree">
                                    <tree>
                                        <field name="alias" editable="bottom"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group>
                                                <group>
                                                </group>
                                                <group>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Telecom">
                                <field name="telecom_ids" mode="tree">
                                    <tree default_order="is_preferred,rank">
                                        <field name="is_preferred"/>
                                        <field name="rank"/>
                                        <field name="use"/>
                                        <field name="telecom_id"/>
                                        <field name="start_date"/>
                                        <field name="end_date"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <h1>
                                                <field name="telecom_id"/>
                                            </h1>
                                            <field name="use" widget="radio" options="{'horizontal': true}"/>
                                            <group>
                                                <group>
                                                    <field name="rank"/>
                                                    <field name="is_preferred"/>
                                                    <field name="is_active"/>
                                                    <field name="start_date" placeholder="Valid From"/>
                                                    <field name="end_date" placeholder="Valid To"/>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Addresses">
                                <field name="address_ids" mode="tree">
                                    <tree default_order="is_preferred">
                                        <field name="is_preferred"/>
                                        <field name="use"/>
                                        <field name="line1"/>
                                        <field name="line2"/>
                                        <field name="city_id"/>
                                        <field name="postal_code_id"/>
                                        <field name="country_id"/>
                                        <field name="start_date"/>
                                        <field name="end_date"/>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <h1>
                                                <field name="address_id" class="oe_no_button"/>
                                            </h1>
                                            <field name="use" widget="radio" options="{'horizontal': true}"/>
                                            <group>
                                                <group>
                                                    <field name="is_preferred"/>
                                                    <field name="is_active"/>
                                                    <field name="start_date" placeholder="Valid From"/>
                                                    <field name="end_date" placeholder="Valid To"/>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Endpoints">
                                <field name="endpoint_ids" mode="tree">
                                    <tree>
<!--                                         <field name="endpoint_id"/> -->
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group>
                                                <group>
<!--                                                     <field name="endpoint_id"/> -->
                                                </group>
                                                <group>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Locations">
                                <field name="location_ids" mode="tree">
                                    <tree>
                                    </tree>
                                    <form>
                                        <sheet>
                                            <group>
                                                <group>
                                                </group>
                                                <group>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Meta">
                                <form>
                                    <sheet>
                                        <group string="Resource">
                                            <group>
                                                <field name="identifier"/>
                                                <field name="implicit_rules"/>
                                                <field name="language_id"/>
                                                <field name="meta_id"/>
                                            </group>
                                            <group>
                                            </group>
                                        </group>
                                        <group string="Domain Resource">
                                            <group>
                                                <field name="text_id"/>
                                            </group>
                                            <group>
                                            </group>
                                        </group>
                                        <notebook>
