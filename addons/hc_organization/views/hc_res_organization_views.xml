        <act_window id="hc_res_organization_action"
                    name="HC Organizations"
                    res_model="hc.res.organization"
                    view_mode="tree,form"
                    domain=""
                    context=""
                    limit=""/>

        <menuitem id="hc_organization.menu_org"
                  name="Organizations"
                  web_icon="hc_organization,static/description/icon.png"
                  parent="hc_base.menu_entity"
                  action="hc_res_organization_action"
                  groups="hc_base.group_hc_base_manager"
                  sequence="10"/>

        <!-- Organization Type Views -->

        <record id="hc_vs_organization_type_view_search" model="ir.ui.view">
            <field name="name">Organization Type Search</field>
            <field name="model">hc.vs.organization.type</field>
            <field name="arch" type="xml">
                <search string="Organization Types">
                    <field name="name"/>
                    <field name="code"/>
                    <field name="definition"/>
                    <group>
                        <filter name="group_by_parent" string="parent" context="{'group_by': 'contains_id'}"/>
                    </group>
                </search>
            </field>
        </record>

        <record id="hc_vs_organization_type_view_tree" model="ir.ui.view">
            <field name="name">Organization Type Tree</field>
            <field name="model">hc.vs.organization.type</field>
            <field name="arch" type="xml">
                <tree string="Organization Types" default_order="name, code">
                    <field name="name"/>
                    <field name="code"/>
                    <field name="definition"/>
                    <field name="contains_id"/>
                    <field name="source_id" invisible="0"/>
                </tree>
            </field>
        </record>

        <record id="hc_vs_organization_type_view_form" model="ir.ui.view">
            <field name="name">Organization Type Form</field>
            <field name="model">hc.vs.organization.type</field>
            <field name="arch" type="xml">
                <form string="Organization Types">
                    <sheet>
                        <h1>
                            <field name="name" default_focus="1" placeholder="Name (e.g., Biological Substance)"/>
                        </h1>
                        <field name="code" placeholder="Code (e.g., biological)"/>
                        <field name="definition" placeholder="Definition"/>
                        <field name="comments" placeholder="Comments"/>
                        <group>
                            <group>
                                <field name="contains_id"/>
                            </group>
                            <group>
                                <field name="source_id" placeholder="e.g., U.S. Census Bureau"/>
                                <field name="system" widget="url" placeholder="e.g., www.census.gov/"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <act_window id="hc_vs_organization_type_action"
                    name="HC Organization Types"
                    res_model="hc.vs.organization.type"
                    view_mode="tree,form"
                    domain=""
                    context=""
                    limit=""/>

        <menuitem id="hc_organization.menu_org_type"
                  name="Organization Types"
                  parent="hc_organization.menu_org"
                  action="hc_vs_organization_type_action"
                  groups="hc_base.group_hc_base_manager"
                  sequence="10"/>

        <!-- Contact Entity Type Views -->

        <record id="hc_vs_contact_entity_type_view_search" model="ir.ui.view">
            <field name="name">Contact Entity Type Search</field>
            <field name="model">hc.vs.contact.entity.type</field>
            <field name="arch" type="xml">
                <search string="Contact Entity Types">
                    <field name="name"/>
                    <field name="code"/>
                    <field name="definition"/>
                    <group>
                        <filter name="group_by_parent" string="parent" context="{'group_by': 'contains_id'}"/>
                    </group>
                </search>
            </field>
        </record>

        <record id="hc_vs_contact_entity_type_view_tree" model="ir.ui.view">
            <field name="name">Contact Entity Type Tree</field>
            <field name="model">hc.vs.contact.entity.type</field>
            <field name="arch" type="xml">
                <tree string="Contact Entity Types" default_order="name, code">
                    <field name="name"/>
                    <field name="code"/>
                    <field name="definition"/>
                    <field name="contains_id"/>
                    <field name="source_id" invisible="0"/>
                </tree>
            </field>
        </record>

        <record id="hc_vs_contact_entity_type_view_form" model="ir.ui.view">
            <field name="name">Contact Entity Type Form</field>
            <field name="model">hc.vs.contact.entity.type</field>
            <field name="arch" type="xml">
                <form string="Contact Entity Types">
                    <sheet>
                        <h1>
                            <field name="name" default_focus="1" placeholder="Name (e.g., Biological Substance)"/>
                        </h1>
                        <field name="code" placeholder="Code (e.g., biological)"/>
                        <field name="definition" placeholder="Definition"/>
                        <field name="comments" placeholder="Comments"/>
                        <group>
                            <group>
                                <field name="contains_id"/>
                            </group>
                            <group>
                                <field name="source_id" placeholder="e.g., U.S. Census Bureau"/>
                                <field name="system" widget="url" placeholder="e.g., www.census.gov/"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <act_window id="hc_vs_contact_entity_type_action"
                    name="HC Contact Entity Types"
                    res_model="hc.vs.contact.entity.type"
                    view_mode="tree,form"
                    domain=""
                    context=""
                    limit=""/>

        <menuitem id="hc_organization.menu_cont_enty_type"
                  name="Contact Entity Types"
                  parent="hc_organization.menu_org"
                  action="hc_vs_contact_entity_type_action"
                  groups="hc_base.group_hc_base_manager"
                  sequence="20"/>

        <!-- External Reference -->

        <record id="hc_vs_identifier_code_view_form_inherited" model="ir.ui.view">
            <field name="name">Identifier Code Form Inherit</field>
            <field name="model">hc.vs.identifier.code</field>
            <field name="inherit_id" ref="hc_base.hc_vs_identifier_code_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form//group//field[@name='type_id']" position="after">
                    <field name="assigner_id"/>
                </xpath>
            </field>
        </record>

