          <field name="model">hc.vs.country.district</field>          
          <field name="arch" type="xml">          
            <form string="District/County">        
              <sheet>     
                <div class="oe_title oe_left">    
                  <h1>  
                    <field name="name" default_focus="1" placeholder="District/County"/>
                  </h1> 
                </div>    
                <group>   
