        </record>

        <record id="hc_vs_country_city_type_view_form" model="ir.ui.view">            
          <field name="name">Country City Type Form</field>          
          <field name="model">hc.vs.country.city.type</field>         
          <field name="arch" type="xml">          
            <form string="City/Place Type">      
              <sheet>     
                <div class="oe_title oe_left">    
                  <h1>  
                    <field name="name" default_focus="1" placeholder="Name (e.g., city, town)"/>
                  </h1> 
                  <field name="code" placeholder="Code (e.g., US)"/>  
                </div>
                <field name="definition" string="Definition" placeholder="Definition"/> 
                <field name="comments" string="Comments" placeholder="Comments"/>    
                <group>   
                  <group> 
                    <field name="is_abstract" widget="upgrade_boolean"/>
                    <field name="level" default="1" placeholder="An integer (e.g., 1, 2)"/>
                    <field name="version" placeholder="e.g., v2.5"/>
                  </group>  
                  <group> 
                    <field name="source" placeholder="e.g., U.S. Census Bureau"/>
                    <field name="system" widget="url" placeholder="e.g., www.census.gov/"/>
                    <field name="country_id"/>
                  </group>  
                </group>    
              </sheet>      
            </form>       
          </field>          
        </record>           
           

        <!-- City Action and Menu --> 

        <record id="hc_vs_country_city_action" model="ir.actions.act_window">           
