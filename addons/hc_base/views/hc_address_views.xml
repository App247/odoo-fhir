<openerp>
    <data>

    <!-- Address Views -->

        <record id="hc_address_view_search" model="ir.ui.view">
          <field name="name">Address Search</field>
          <field name="model">hc.address</field>
          <field name="arch" type="xml">
            <search string="Addresses">
              <field name="name"/>
              <field name="postal_code_id" string="Postal/ZIP Code"/>
              <field name="city_id" string="City/Place"/>
              <field name="district_id" string="District/County"/>
              <field name="state_id" string="State/Province"/>
              <field name="division_id"/>
              <field name="region_id"/>
              <field name="country_id"/>

              <filter name="state_id" string="California" domain="[('state_id', '=', 'base.state_us_5')]"/> 
              <group string="Group By"> 
                <filter name="group_by_state" string="State" context="{'group_by': 'state_id'}"/>
              </group> 
            </search>
          </field>
        </record>

        <record id="hc_address_view_tree" model="ir.ui.view">
          <field name="name">Address Tree</field>
          <field name="model">hc.address</field>
          <field name="arch" type="xml">
            <tree string="Addresses">
              <field name="line1" string="Street"/>
              <field name="city_id" string="City/Place"/>
              <field name="postal_code_id" string="Postal/ZIP Code"/>
              <field name="district_id" string="District/County"/>
              <field name="state_id" string="State/Province"/>
              <field name="division_id"/>
              <field name="region_id"/>
            </tree>
          </field>
        </record>

    <record id="hc_address_view_form" model="ir.ui.view">
          <field name="name">Address Form</field>
          <field name="model">hc.address</field>
          <field name="arch" type="xml">
            <form string="Address">
              <sheet>
                <div class="oe_title oe_left">
                  <h1>
                    <field name="name" default_focus="1" readonly="1" placeholder="Full Address"/>
                  </h1>
                </div>
                <group>
                  <group>
                    <field name="line1"/>
                    <field name="line2"/>
                    <field name="line3"/>
                    <field name="city_id" string="City/Place"/>
                    <field name="postal_code_id" string="Postal/ZIP Code"/>
                  </group>
                  <group>
                    <field name="district_id" string="District/County"/>
                    <field name="state_id" placeholder="State/Province"/>
                    <field name="division_id"/>
                    <field name="region_id"/>
                    <field name="country_id"/>
                  </group>
                </group>
              </sheet>
            </form>
          </field>
        </record>

        <!-- Address Action -->   
        
          <record id="hc_address_action" model="ir.actions.act_window">
            <field name="name">HC Addresses</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">hc.address</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
          </record>
        
        <!-- Address Menu Item -->

        <menuitem name="Address" id="hc_base.menu_addr" parent="hc_base.menu_datatype" sequence="10"/>
        <menuitem name="Addresses" id="hc_base.menu_addr_addr" parent="hc_base.menu_addr" action="hc_address_action" sequence="10"/>

        <!-- Postal/ZIP Code Views -->

        <record id="hc_vs_country_postal_code_view_search" model="ir.ui.view">      
          <field name="name">Postal Code Search</field>    
          <field name="model">hc.vs.country.postal.code</field>   
          <field name="arch" type="xml">    
            <search string="Postal/ZIP Codes"> 
              <field name="name"/>
              <field name="postal_place"/>
              <field name="city_ids" string="Cities/Places"/>
              <field name="district_ids" string="District/County"/>
              <field name="state_ids" string="State/Province"/>
              <field name="division_id"/>
              <field name="region_id"/>
              <field name="country_id"/>
            </search> 
          </field>    
        </record>     

        <record id="hc_vs_country_postal_code_view_tree" model="ir.ui.view">     
          <field name="name">Postal Code Tree</field>    
          <field name="model">hc.vs.country.postal.code</field>   
          <field name="arch" type="xml">   
            <tree string="Postal/ZIP Codes">
              <field name="name" string="Code"/>
              <field name="postal_place"/>
              <field name="city_ids" string="Cities/Places"/>
              <field name="district_ids" string="Districts/Counties"/>
              <field name="state_ids" string="State/Province"/>
              <field name="division_id" invisible="1"/>
              <field name="region_id" invisible="1"/>
            </tree> 
          </field>    
        </record>     

        <record id="hc_vs_country_postal_code_view_form" model="ir.ui.view">          
          <field name="name">Postal Code Form</field>        
          <field name="model">hc.vs.country.postal.code</field>       
          <field name="arch" type="xml">        
            <form string="Postal/ZIP Code">      
              <sheet>   
                <div class="oe_title oe_left">
                  <h1>
                    <field name="name" default_focus="1" placeholder="Postal or ZIP Code"/>
                  </h1>
                  <field name="postal_place" placeholder="(optional) Place like Hillsdale Shopping Center"/>
                </div>
                  <group>   
                    <group>   
                      <field name="city_ids" widget="many2many_tags" string="Cities/Places"/>
                      <field name="district_ids" widget="many2many_tags" string="Districts/Counties"/>   
                      <field name="state_ids" widget="many2many_tags" string="States/Provinces"/>   
                    </group>
                    <group>    
                      <field name="division_id"/>   
                      <field name="region_id"/>   
                      <field name="country_id"/>    
                    </group>    
                  </group>    
              </sheet>    
            </form>     
          </field>        
        </record>         

        <!-- Postal/ZIP Code Action and Menu--> 

        <record id="hc_vs_country_postal_code_action" model="ir.actions.act_window">  
          <field name="name">HC Postal/ZIP Codes</field>
          <field name="type">ir.actions.act_window</field>
          <field name="res_model">hc.vs.country.postal.code</field>
          <field name="view_type">form</field>
          <field name="view_mode">tree,form</field>
        </record> 

        <menuitem name="Postal/ZIP Codes" id="hc_base.menu_addr_post" parent="hc_base.menu_addr" action="hc_vs_country_postal_code_action" sequence="20"/>

        <!-- City Views -->           
                  
