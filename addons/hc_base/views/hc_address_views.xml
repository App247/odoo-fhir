                <filter name="group_by_division" string="Division" context="{'group_by': 'division_id'}"/>
                <filter name="group_by_region" string="Region" context="{'group_by': 'region_id'}"/>
                <filter name="group_by_country" string="Country" context="{'group_by': 'country_id'}"/>
              </group>
            </search>       
          </field>
        </record>           
                    
        <record id="hc_vs_country_district_view_tree" model="ir.ui.view">           
          <field name="name">Country District Tree</field>         
          <field name="model">hc.vs.country.district</field>          
