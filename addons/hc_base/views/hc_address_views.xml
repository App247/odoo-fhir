        </record>     
     
        <!-- District Views -->           
                
        <record id="hc_vs_country_district_view_search" model="ir.ui.view">           
          <field name="name">Country District Search</field>         
          <field name="model">hc.vs.country.district</field>          
          <field name="arch" type="xml">          
