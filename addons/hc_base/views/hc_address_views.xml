          <field name="model">hc.vs.country.division</field>          
          <field name="arch" type="xml">          
            <form string="Division">       
              <sheet>     
                <div class="oe_title oe_left">    
                  <h1>  
                    <field name="name" default_focus="1" placeholder="Division"/>
                  </h1> 
                </div>    
                <group>   
                  <group>
                    <field name="code"/>
                    <field name="type_id"/>
                  </group>
                  <group> 
                    <field name="region_id"/>
                    <field name="country_id"/>
                  </group>  
                  <group> 
                  </group>  
