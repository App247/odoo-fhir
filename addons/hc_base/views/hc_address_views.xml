                    <field name="type_id"/>
                    <field name="category_id"/>
                    <field name="status_id"/>
                  </group>  
                  <group>
                    <field name="postal_code_ids" widget="many2many_tags" string="Postal/ZIP Codes"/>
                    <field name="district_ids" widget="many2many_tags" string="District/County"/>
                    <field name="state_id" string="State/Province"/> 
                    <field name="division_id"/>
                    <field name="region_id"/>
                    <field name="country_id"/>
                  </group>  
                </group>    
              </sheet>      
            </form>       
          </field>          
        </record>
                     
        <record id="hc_vs_country_city_type_view_search" model="ir.ui.view">           
          <field name="name">Country City Type Search</field>         
          <field name="model">hc.vs.country.city.type</field>          
